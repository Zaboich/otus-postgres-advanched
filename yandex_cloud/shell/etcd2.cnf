ETCD_NAME="etcd1"
ETCD_LISTEN_PEER_URLS="http://127.0.0.1:2380,http://192.168.0.16:2380"
ETCD_LISTEN_CLIENT_URLS="http://127.0.0.1:2379,http://192.168.0.16:2379"

ETCD_INITIAL_CLUSTER="etcd1=http://etcd1:2380"
